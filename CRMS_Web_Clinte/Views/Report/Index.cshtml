@model IEnumerable<CRMS_Web_Clinte.Models.AllCarReport>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VehicleId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VehicleName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Photo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalReserved)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalEarn)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.VehicleId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VehicleName)
        </td>
        <td>
            <span style="height: 150px;width: 150px;"><img id="Photo1" src="data:image/gif;base64,@Html.DisplayFor(modelItem => item.Photo)" style="height: 150px; width: 150px;" /></span>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalReserved)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalEarn)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id = item.VehicleId }) 
        </td>
    </tr>
}

</table>
